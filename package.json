{
  "name": "website",
  "version": "1.0.0",
  "description": "My website, built with 11ty",
  "main": "index.js",
  "scripts": {
    "start": "eleventy --serve",
    "start:src": "parcel watch --out-dir site/static --public-url /static/ 'src/*.(js|css)'",
    "build": "npm run build:src && npm run build:site",
    "build:site": "eleventy",
    "build:src": "parcel build --out-dir site/static --public-url /static/ 'src/*.(js|css)'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrsphil/website.git"
  },
  "author": "Andreas Philippi",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/andrsphil/website/issues"
  },
  "homepage": "https://github.com/andrsphil/website#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.1.4",
    "tailwindcss-transition": "^1.0.5"
  },
  "dependencies": {
    "turbolinks": "^5.2.0"
  }
}
